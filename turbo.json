{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "cache": true,
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "clean": {
            "cache": false
        },
        "test": {
            "cache": true,
            "dependsOn": [
                "^build"
            ],
            "outputs": []
        },
        "test:watch": {
            "cache": true,
            "dependsOn": [
                "^build"
            ],
            "outputs": []
        },
        "test:coverage": {
            "cache": true,
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "coverage/**"
            ]
        }
    }
}