{
    "name": "@repo/types",
    "version": "0.4.0",
    "description": "TypeScript type definitions for the Caly Xano tooling ecosystem",
    "private": true,
    "publishConfig": {
        "access": "restricted"
    },
    "license": "MIT",
    "author": "Mihály Tóth | @calycode",
    "keywords": [
        "xano",
        "cli",
        "types",
        "typescript",
        "api",
        "backend"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.bundled.d.ts"
        }
    },
    "types": "dist/index.bundled.d.ts",
    "type": "module",
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md"
    ],
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/calycode/xano-tools.git",
        "directory": "packages/types"
    },
    "bugs": {
        "url": "https://github.com/calycode/xano-tools/issues"
    },
    "homepage": "https://github.com/calycode/xano-tools/tree/main/#readme",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "clean": "trash dist tsconfig.tsbuildinfo",
        "build:types": "tsc",
        "bundle:types": "rollup -c rollup.config.js",
        "postbuild": "tsx ../../scripts/clean-types.ts . index.bundled.d.ts",
        "build": "pnpm clean && pnpm build:types && pnpm bundle:types && pnpm postbuild"
    }
}