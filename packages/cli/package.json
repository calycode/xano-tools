{
    "name": "@mihalytoth20/xano-community-cli",
    "version": "0.1.1",
    "description": "Command-line interface for the Xano Community CLI providing terminal access to Xano workflows",
    "keywords": ["xano", "cli", "command-line", "api", "openapi", "backend", "automation", "developer-tools"],
    "bin": {
        "xcc": "dist/index.cjs"
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist",
        "README.md"
    ],
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MihalyToth20/xano-community-cli.git",
        "directory": "packages/cli"
    },
    "bugs": {
        "url": "https://github.com/MihalyToth20/xano-community-cli/issues"
    },
    "homepage": "https://github.com/MihalyToth20/xano-community-cli#readme",
    "dependencies": {
        "@clack/prompts": "^0.11.0",
        "@mihalytoth20/xcc-core": "workspace:*",
        "@mihalytoth20/xcc-types": "workspace:*",
        "@mihalytoth20/xcc-utils": "workspace:*",
        "chalk": "^5.4.1",
        "commander": "^14.0.0",
        "js-yaml": "^4.1.0",
        "tar": "^7.4.3"
    },
    "scripts": {
        "build": "tsc --build && tsx esbuild.config.ts && shx chmod +x dist/index.cjs",
        "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
        "xcc": "node dist/index.cjs"
    },
    "devDependencies": {
        "shx": "^0.4.0",
        "uvu": "^0.5.6"
    }
}
