{
   "name": "@mihalytoth20/xano-community-cli",
   "version": "0.1.1",
   "main": "dist/index.cjs",
   "type": "module",
   "description": "Supercharge your Xano workflow: automate backups, docs, testing, and version control—no AI guesswork, just reliable, transparent dev tools.",
   "bin": {
      "xcc": "dist/index.cjs"
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "xcc": "tsx src/cli/index.ts",
      "xano-community-cli": "tsx src/cli/index.ts",
      "update-xcc-docs": "tsx scripts/generate-xcc-docs.ts",
      "build": "pnpm update-xcc-docs && tsx scripts/build.ts",
      "prepublishOnly": "pnpm run build",
      "serve-registry": "npx serve registry --listen 3030"
   },
   "private": true,
   "publishConfig": {
      "access": "restricted"
   },
   "keywords": [
      "Xano",
      "cli",
      "xano-community-cli",
      "dev-tool"
   ],
   "author": "Mihály Tóth <hello@nextcurve.hu>",
   "license": "MIT",
   "dependencies": {
      "@clack/prompts": "^0.11.0",
      "axios": "^1.11.0",
      "chalk": "^5.4.1",
      "cli-progress": "^3.12.0",
      "commander": "^14.0.0",
      "form-data": "^4.0.4",
      "js-yaml": "^4.1.0",
      "tar": "^7.4.3"
   },
   "devDependencies": {
      "@eslint/js": "^9.32.0",
      "@typescript-eslint/eslint-plugin": "^8.39.1",
      "@typescript-eslint/parser": "^8.39.1",
      "esbuild": "^0.25.9",
      "eslint": "^9.12.0",
      "eslint-plugin-import": "^2.32.0",
      "knip": "^5.62.0",
      "strip-ansi": "^7.1.0",
      "tsx": "^4.20.4",
      "uvu": "^0.5.6"
   }
}