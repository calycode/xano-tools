{
   "name": "@mihalytoth20/xano-community-cli-workspace",
   "version": "0.1.1",
   "type": "module",
   "description": "Supercharge your Xano workflow: automate backups, docs, testing, and version control—no AI guesswork, just reliable, transparent dev tools.",
   "workspaces": [
      "packages/cli",
      "packages/core",
      "packages/types",
      "packages/utils"
   ],
   "scripts": {
      "build": "tsc --build",
      "build:packages": "pnpm -r --workspace-concurrency=1 run build",
      "xcc": "pnpm --filter @mihalytoth20/xano-community-cli run xcc",
      "update-xcc-docs": "tsx scripts/generate-xcc-docs.ts",
      "serve-registry": "npx serve registry --listen 3030",
      "clean": "pnpm -r run clean && rm -rf dist",
      "prepublishOnly": "pnpm run build:packages"
   },
   "private": true,
   "publishConfig": {
      "access": "restricted"
   },
   "keywords": [
      "Xano",
      "cli",
      "xano-community-cli",
      "dev-tool"
   ],
   "author": "Mihály Tóth <hello@nextcurve.hu>",
   "license": "MIT",
   "dependencies": {},
   "devDependencies": {
      "@eslint/js": "^9.32.0",
      "@types/node": "^24.3.0",
      "@typescript-eslint/eslint-plugin": "^8.39.1",
      "@typescript-eslint/parser": "^8.39.1",
      "esbuild": "^0.25.9",
      "eslint": "^9.12.0",
      "eslint-plugin-import": "^2.32.0",
      "knip": "^5.62.0",
      "strip-ansi": "^7.1.0",
      "tsx": "^4.20.4",
      "typescript": "^5.9.2"
   }
}
